name: Toyota_Price_Prediction_CI

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: 6}
      learning_rate: {type: float, default: 0.1}
      test_size: {type: float, default: 0.2}
    command: "python modelling.py --n_estimators {n_estimators} --max_depth {max_depth} --learning_rate {learning_rate} --test_size {test_size}"
